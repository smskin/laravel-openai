{
    "name": "smskin/laravel-opeai",
    "description": "OpenAI API wrapper for laravel projects",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel",
        "openai",
        "ai",
        "api"
    ],
    "authors": [
        {
            "name": "Sergey Mikhailov (smskin)",
            "email": "sms7@yandex.ru"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10 || ^11",
        "openai-php/client": "dev-main"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.62",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^8 || ^9",
        "phpunit/phpunit": "^10.5",
        "vimeo/psalm": "^5.25"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "SMSkin\\LaravelOpenAi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SMSkin\\LaravelOpenAi\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "php-http/discovery": true
        },
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SMSkin\\LaravelOpenAi\\Providers\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "analyze": "psalm",
        "check-style": [
            "php-cs-fixer fix --diff --dry-run",
            "php-cs-fixer fix --diff --dry-run --config=.php-cs-fixer.tests.php"
        ],
        "fix-style": [
            "php-cs-fixer fix",
            "php-cs-fixer fix --config=.php-cs-fixer.tests.php"
        ],
        "psalm-set-baseline": "psalm --set-baseline=psalm-baseline.xml",
        "test": "phpunit"
    }
}
